#include <stdio.h>
#include <stdlib.h>

/**
 * main - multiplies two numbers
 * @argc: argument count
 * @argv: argument vector
 * Return: 0 on success, 1 on error
 */
int main(int argc, char *argv[])
{
int num1, num2, resultat;

if (argc != 3)
{
	printf("Error\n");
	return (1);
}
/*atoi convertit une chaine en entier*/
num1 = atoi(argv[1]); 
num2 = atoi(argv[2]);
result = num1 * num2;
printf("%d\n", result);
return (0);
}
