#include <stdio.h>
11;rgb:2b2b/2b2b/2b2b
/**
 * main - prints all single-digit numbers separated by ", "
 * Return: Always 0 (Success)
 */
int main(void)
{
int n;


for (n = 0; n < 10; n++)
{
putchar(n + '0');
if (n != 9)
{
	putchar(',');
	putchar(' ');
}
}
 putchar('$');
putchar('\n');
return (0);
}
