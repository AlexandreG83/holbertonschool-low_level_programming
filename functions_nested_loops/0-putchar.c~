#include <stdio.h>

/**
 * main - prints all single-digit numbers separated by ", ", ends with $
 * Return: Always 0 (Success)
 */
int main(void)
{
	putchar('_');
	putchar('p');
	putchar('u');
	putchar('t');
	putchar('c');
	putchar('h');
	putchar('a');
	putchar('r');
	putchar('\n');

	return (0);
}
